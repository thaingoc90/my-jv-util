<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


	<bean id="_languageLoader" class="ind.web.nhp.base.LanguageLoader"
		init-method="init">
		<property name="configLocation" value="/WEB-INF/languages/locale-config.properties" />
	</bean>

	<bean id="_applicationContextUtils" class="ind.web.nhp.utils.SpringUtils"></bean>
</beans>